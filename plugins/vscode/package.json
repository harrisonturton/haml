{
  "name": "haml",
  "displayName": "haml",
  "description": "Syntax highlighting for the HAML markup language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "haml",
      "aliases": ["haml", "haml"],
      "extensions": ["haml", "haml.spec"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "haml",
      "scopeName": "source.haml",
      "path": "./syntaxes/haml.tmLanguage.json"
    }]
  }
}
